#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/willstreeter/WebstormProjects/vibe-coding/shifu-project/Discourser-Design-System/node_modules/.pnpm/@pandacss+dev@0.52.0_jsdom@27.4.0_typescript@5.9.3/node_modules/@pandacss/dev/node_modules:/Users/willstreeter/WebstormProjects/vibe-coding/shifu-project/Discourser-Design-System/node_modules/.pnpm/@pandacss+dev@0.52.0_jsdom@27.4.0_typescript@5.9.3/node_modules/@pandacss/node_modules:/Users/willstreeter/WebstormProjects/vibe-coding/shifu-project/Discourser-Design-System/node_modules/.pnpm/@pandacss+dev@0.52.0_jsdom@27.4.0_typescript@5.9.3/node_modules:/Users/willstreeter/WebstormProjects/vibe-coding/shifu-project/Discourser-Design-System/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/willstreeter/WebstormProjects/vibe-coding/shifu-project/Discourser-Design-System/node_modules/.pnpm/@pandacss+dev@0.52.0_jsdom@27.4.0_typescript@5.9.3/node_modules/@pandacss/dev/node_modules:/Users/willstreeter/WebstormProjects/vibe-coding/shifu-project/Discourser-Design-System/node_modules/.pnpm/@pandacss+dev@0.52.0_jsdom@27.4.0_typescript@5.9.3/node_modules/@pandacss/node_modules:/Users/willstreeter/WebstormProjects/vibe-coding/shifu-project/Discourser-Design-System/node_modules/.pnpm/@pandacss+dev@0.52.0_jsdom@27.4.0_typescript@5.9.3/node_modules:/Users/willstreeter/WebstormProjects/vibe-coding/shifu-project/Discourser-Design-System/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
